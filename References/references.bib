@article{Torres2012,
  abstract = {The recent interest in the development of small unmanned aerial vehicles (UAVs) and micro air vehicles has revealed a need for a more thorough understanding of the aerodynamics of small airplanes flying at low speeds. In response to this need, a study of the lift, drag, and pitching moment characteristics of wings of low aspect ratio operating at low Reynolds numbers are presented. Wind-tunnel tests of wings with aspect ratios between 0.5 and 2.0, four distinct planforms, thickness-to-chord ratios of ≈ 2\%, and 5-to-1 elliptical leading edges have been conducted as part of this research. The Reynolds numbers considered were in the range of 7 × 104 to 2 × 10. Analysis of the data includes comparison of lift-curve slope, nonlinear equation approximations, maximum lift coefficient, and center of lift.},
  author = {Gabriel E. Torres and Thomas J. Mueller},
