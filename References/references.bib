  keywords = {Aerodynamic Characteristics,Aeroplane,High Aspect Ratio,Lift Coefficient,Mean Aerodynamic Chord,Nonlinear Equation,Remotely Piloted Aircraft,Thickness to Chord Ratio,Wind Tunnel Tests,Wingtip Vortices},
  month = {5},
