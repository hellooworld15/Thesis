@article{Aboelezz2020,
  abstract = {In this paper, the design process of a fixed-wing Micro Air Vehicle (MAV) is presented. The design cycle begins with mission requirements, followed by collecting statistical data from available MAVs. Next, MAV's sizing and weight estimation are conducted based on mission requirements. Finally, the aspect ratio, taper ratio, and airfoil selection are performed. As the MAV has a low aspect ratio and low Reynolds number airflow, the aerodynamics is not fully understood. The propeller wash increases the complexity of the aerodynamic flow; therefore, the MAV is manufactured for wind tunnel testing. The absent of actual flight performance and flight dynamics of fixed-wing MAVs are the main driven factors for this research. The wind tunnel experimental data are used in the flight performance equations to evaluate the MAV's actual flight performance and compare it with the estimated values. As MAV is very sensitive to the external disturbances which make the prediction of its dynamics a hard task, a nonlinear flight simulation model is created for the MAV's dynamics prediction. This model uses the experimentally measured data to predict the MAV's dynamics and its stability against wind disturbances. The results in this paper indicate that using aerodynamics historical data will give a huge error in estimating MAV's flight performance. The measured data in this research can be used to eliminate this error. Also, the flight dynamics of fixed-wing MAVs cannot be assumed linear, and the technique used in this paper gives a very accurate flight dynamics estimation.},
  author = {A. Aboelezz and M. Hassanalian and A. Desoki and B. Elhadidi and G. El-Bayoumi},
  doi = {10.1016/J.AST.2019.105636},
  issn = {1270-9638},
  journal = {Aerospace Science and Technology},
  keywords = {Flight dynamics,MAV design,Nonlinear dynamics,Simulink,Sizing,Wind tunnel testing},
  month = {2},
  pages = {105636},
  publisher = {Elsevier Masson},
  title = {Design, experimental investigation, and nonlinear flight dynamics with atmospheric disturbances of a fixed-wing micro air vehicle},
  volume = {97},
  year = {2020},
}
@article{Keennon2003,
  abstract = {In the future, MAVs will most likely emerge in operational use in fixed wing, rotary wing, and finally, flapping wing configurations. This is due to the inherent difficulties in optimizing rotary and flapping wing designs. The three classes of designs will emerge with varying capabilities, and for a time, the fixed wing systems will have the greatest endurance for a given mass. But then ornithopters or flapping wing aircraft may become the dominant configuration as wingspans shrink and requirements evolve, with spans of less than 1 cm, and masses less than 1 gram. This is where biological flyers can show us how to best operate in those extremely low Reynolds numbers regimes. AeroVironment has developed some of the early pioneering MAVs, which were very useful in developing tools for understanding and optimizing these diminutive flyers. The Microbat MAV is a 23 cm span, electric, remotely piloted, flapping wing ornithopter capable of flying for over 22 minutes. The Black Widow MAV is a 15 cm span, electric, remotely piloted flying wing with a color video payload that can operate at a 2 km range with an endurance of 30 minutes. 1 Through careful analysis of propulsion systems, energy storage systems, and aerodynamic design, optimization tools can be developed and utilized to design MAVs with surprisingly high performance. This paper examines the current state of the art of several MAV subsystem technologies, provides a near-term outlook for the future, and suggests areas that could benefit from further research.},
  author = {Matthew Keennon and Joel Grasmeyer},
  doi = {10.2514/6.2003-2901},
  month = {7},
  publisher = {American Institute of Aeronautics and Astronautics (AIAA)},
  title = {Development of Two MAVs and Vision of the Future of MAV Design},
  url = {https://arc.aiaa.org/doi/pdf/10.2514/6.2003-2901},
  year = {2003},
  journal = {American Institute of Aeronautics and Astronautics AIAA International Air and Space Symposium and Exposition: The Next 100 Years - Dayton, Ohio (14 July 2003 - 17 July 2003)}
}
