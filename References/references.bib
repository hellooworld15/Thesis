  keywords = {Aerodynamic Characteristics,Air Vehicle,Critical Angle of Attack,Direct Numerical Simulation,Incompressible Flow,Lift to Drag Ratio,Propeller Blades,Strouhal Number,Triangular Airfoil,Wind Tunnel Tests},
  month = {12},
  pages = {924-931},
